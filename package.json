{
  "private": true,
  "name": "serveside",
  "version": "0.1.0-alpha.1",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "changelog": "lerna-changelog",
    "build": "lerna run build --parallel",
    "lint": "lerna run lint --parallel",
    "test": "cross-env FORCE_COLOR=true jest",
    "docs": "jsdoc -c ./jsdoc.json -r",
    "commit": "cz",
    "prepare": "husky install",
    "version": "lerna version",
    "versionup": "npm run version -- --conventional-commits --no-git-tag-version",
    "versionup:prerelease": "npm run version -- prerelease --conventional-commits --no-git-tag-version",
    "versionup:patch": "npm run version -- patch --conventional-commits --no-git-tag-version",
    "versionup:minor": "npm run version -- minor --conventional-commits --no-git-tag-version",
    "versionup:major": "npm run version -- major --conventional-commits --no-git-tag-version",
    "postversionup": "npm run commit-version",
    "postversionup:prerelease": "npm run commit-version",
    "postversionup:patch": "npm run commit-version",
    "postversionup:minor": "npm run commit-version",
    "postversionup:major": "npm run commit-version",
    "commit-version": "HUSKY=0 git add . && git commit --no-verify -m \"chore(release): publish `node -p 'require(\"./lerna.json\").version'`\"",
    "predeploy": "npm run build",
    "deploy": "lerna publish",
    "release": "npm run deploy -- from-package",
    "release:canary": "npm run deploy -- --canary"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/serveside/serveside.git"
  },
  "engines": {
    "yarn": ">=1 <2",
    "node": ">=10"
  },
  "dependencies": {
    "lerna": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/config-lerna-scopes": "^13.1.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "babel-plugin-nodejs-import-images": "^1.0.0",
    "commitizen": "^4.2.4",
    "core-js": "^3.16.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "docdash": "^1.2.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "eslint-plugin-vue": "^7.16.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jsdoc": "^3.6.7",
    "lerna-changelog": "^2.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "svelte": "^3.42.1",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14"
  }
}
